{
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@7.29.3",
    "scripts": {
        "dev": "nuxi dev",
        "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
        "lint": "eslint . --fix",
        "build": "nuxi build",
        "start": "node .output/server/index.mjs",
        "preinstall": "npx only-allow pnpm",
        "postinstall": "nuxi prepare",
        "generate": "nuxi generate",
        "start:generate": "pnpx serve .output/public",
        "prepare": "husky install",
        "commit": "git-cz",
        "release": "release-it",
        "prisma-push": "pnpx prisma db push"
    },
    "dependencies": {
        "@cooj/utils": "^0.0.1",
        "@prisma/client": "^5.6.0",
        "crypto-js": "^4.2.0",
        "element-plus": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "nuxt-swiper": "^1.2.2",
        "release-it": "^17.0.0",
        "vue-draggable-plus": "^0.3.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@antfu/eslint-config": "2.1.0",
        "@cooj/eslint-config-vue": "^0.1.3",
        "@element-plus/nuxt": "^1.0.7",
        "@iconify-json/carbon": "^1.1.23",
        "@iconify-json/ep": "^1.1.13",
        "@iconify-json/ic": "^1.1.16",
        "@iconify-json/mdi": "^1.1.57",
        "@iconify-json/twemoji": "^1.1.14",
        "@nuxtjs/color-mode": "^3.3.2",
        "@nuxtjs/i18n": "8.0.0-rc.2",
        "@pinia/nuxt": "^0.5.1",
        "@types/crypto-js": "^4.2.1",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/ws": "^8.5.10",
        "@unocss/eslint-config": "^0.57.7",
        "@unocss/nuxt": "^0.57.7",
        "@unocss/preset-rem-to-px": "^0.57.7",
        "@vite-pwa/nuxt": "^0.3.2",
        "@vueuse/nuxt": "^10.6.1",
        "changelogen": "^0.5.5",
        "commitizen": "^4.3.0",
        "cz-git": "^1.7.1",
        "eslint": "^8.54.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "nuxt": "^3.8.2",
        "pinia": "^2.1.7",
        "prisma": "^5.6.0",
        "sass": "^1.69.5",
        "typescript": "^5.3.2"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{htm,html,css,sss,less,scss,sass}": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
